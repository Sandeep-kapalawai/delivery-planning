{
    "name": "delivery-planning",
    "version": "1.2.9",
    "description": "custom setup of vue + webpack + module federation + linting + more...",
    "main": "main.js",
    "engines": {
        "node": "16.17.0",
        "npm": "8.19.2"
    },
    "scripts": {
        "mock": "nodemon --watch ./mock-server ./mock-server/index.js",
        "serve": "webpack serve --mode=none --env NODE_ENV=local --port 8093",
        "serve:mock": "concurrently \"npm run mock\" \"npm run serve\"",
        "serve:build:local": "concurrently \"npm run build:local\" \"serve ./dist -p 8093\"",
        "serve:build:development": "concurrently \"npm run build:development\" \"serve ./dist -p 8093\"",
        "serve:build:production": "concurrently \"npm run build:production\" \"serve ./dist -p 8093\"",
        "build": "webpack --mode=production --env NODE_ENV=production ENV_FILE=template OUTPUT_DIRECTORY=$APP_OUTPUT_DIRECTORY",
        "build:local": "webpack --mode=none --env NODE_ENV=local OUTPUT_DIRECTORY=$APP_OUTPUT_DIRECTORY",
        "build:development": "webpack --mode=development --env NODE_ENV=development OUTPUT_DIRECTORY=$APP_OUTPUT_DIRECTORY",
        "build:production": "webpack --mode=production --env NODE_ENV=production OUTPUT_DIRECTORY=$APP_OUTPUT_DIRECTORY",
        "build:stats": "webpack-bundle-analyzer ./dist/stats.json",
        "lint": "eslint \"src/**/*.{js,ts,vue}\"",
        "lint:fix": "npm run lint -- --fix",
        "lint:print": "npm run lint > linting-output.txt",
        "lint:styles": "stylelint \"src/**/*.{scss,vue}\"",
        "lint:styles:fix": "npm run lint:styles -- --fix",
        "lint:styles:print": "npm run lint:styles > linting-styles-output.txt",
        "lint:all": "concurrently \"npm run lint\" \"npm run lint:styles\"",
        "lint:all:fix": "concurrently \"npm run lint:fix\" \"npm run lint:styles:fix\"",
        "lint:all:print": "npm run lint:all > linting-all-output.txt",
        "test": "npm run test:unit:coverage",
        "test:unit": "node --max_old_space_size=8192 node_modules/.bin/jest",
        "test:unit:watch": "npm run test:unit -- --watchAll",
        "test:unit:coverage": "npm run test:unit -- --coverage",
        "test:performance": "rm -rf .lighthouseci && concurrently \"npm run mock\" \"lhci autorun\""
    },
    "keywords": [
        "vue",
        "2.x",
        "webpack",
        "module",
        "federation",
        "linting",
        "sass",
        "sass-loader"
    ],
    "author": "Ehtezaz Shah",
    "license": "ISC",
    "dependencies": {
        "@maersk-global/mds-components-community": "^1.58.0",
        "@maersk-global/mds-components-core": "^1.58.0",
        "@maersk-global/telemetry-web-sdk": "1.0.13",
        "@scm-common/federation": "1.1.7",
        "@scm-ui/column-configurator": "^1.9.9",
        "@scm-ui/date-picker": "^0.5.4",
        "@scm-ui/details-card": "^0.3.7",
        "@scm-ui/filters": "^1.8.2",
        "@scm-ui/grid-system": "^1.1.2",
        "@scm-ui/overflow-menu": "^1.3.1",
        "@scm-ui/page-header": "^1.12.15",
        "@scm-ui/page-layout": "^0.3.0",
        "@scm-ui/party-card": "^0.2.7",
        "@scm-ui/preview-panel": "^1.3.7",
        "@scm-ui/table": "^1.55.4",
        "@scm-ui/table-title-bar": "^1.16.1",
        "@scm-ui/tools-panel": "^1.10.4",
        "axios": "^0.27.2",
        "buffer": "^6.0.3",
        "core-js": "3.25.3",
        "date-fns": "^2.30.0",
        "jest-junit": "^15.0.0",
        "lodash": "^4.17.21",
        "stream-browserify": "^3.0.0",
        "tippy.js": "^6.3.7",
        "vee-validate": "^3.4.14",
        "vue": "2.6.14",
        "vue-i18n": "^8.28.2",
        "vue-router": "3.5.2",
        "vuex": "3.6.2"
    },
    "devDependencies": {
        "@babel/core": "7.15.5",
        "@babel/eslint-parser": "7.15.7",
        "@babel/preset-env": "7.15.6",
        "@lhci/cli": "0.8.2",
        "@types/jest": "27.4.0",
        "@types/lodash": "^4.14.185",
        "@types/node": "17.0.16",
        "@typescript-eslint/eslint-plugin": "5.11.0",
        "@typescript-eslint/parser": "5.11.0",
        "@vue/eslint-config-typescript": "9.1.0",
        "@vue/test-utils": "1.2.2",
        "@vue/vue2-jest": "27.0.0-alpha.2",
        "babel-loader": "8.2.2",
        "clean-webpack-plugin": "4.0.0",
        "concurrently": "7.4.0",
        "copy-webpack-plugin": "^11.0.0",
        "cors": "2.8.5",
        "css-loader": "6.2.0",
        "dotenv-webpack": "7.0.3",
        "eslint": "7.32.0",
        "eslint-plugin-vue": "8.0.1",
        "eslint-webpack-plugin": "3.2.0",
        "flush-promises": "^1.0.2",
        "html-webpack-plugin": "5.3.2",
        "jest": "27.3.1",
        "jest-fetch-mock": "3.0.3",
        "jest-sonar": "^0.2.16",
        "jest-sonar-reporter": "2.0.0",
        "mini-css-extract-plugin": "2.5.3",
        "nodemon": "2.0.20",
        "postcss-loader": "6.1.1",
        "process": "0.11.10",
        "sass": "1.41.1",
        "sass-loader": "12.1.0",
        "serve": "12.0.1",
        "stylelint": "13.13.1",
        "stylelint-config-sass-guidelines": "8.0.0",
        "terser-webpack-plugin": "5.2.4",
        "ts-jest": "27.1.3",
        "ts-loader": "9.2.6",
        "typescript": "4.5.5",
        "vue-loader": "15.9.8",
        "vue-remove-attributes": "1.0.3",
        "vue-template-compiler": "2.6.14",
        "webpack": "5.74.0",
        "webpack-bundle-analyzer": "4.6.1",
        "webpack-cli": "4.10.0",
        "webpack-dev-server": "4.11.1"
    },
    "jestSonar": {
        "reportPath": "test-reports",
        "reportFile": "test-report.xml"
    }
}
